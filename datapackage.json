    "format": "csv",
    "schema": {
      "fields": [{
          "name": "Town",
          "type": "string",
          "dimension": false
        }, {
          "name": "FIPS",
          "type": "string",
          "dimension": false
        }, {
          "name": "Year",
          "type": "string",
          "dimension": false
        }, {
          "name": "Age Range",
          "type": "string",
          "dimension": true,
          "constraints": {
            "enum": ["All", "Under 18", "18-64", "65+"]
         }
        }, {
          "name": "Insurance Status",
          "type": "string",
          "dimension": true,
          "constraints": {
            "enum": ["Without Health Insurance",
                     "With Private Health Insurance",
                     "With Public Health Insurance"]
         }
        }, {
          "name": "Measure Type",
          "type": "string",
          "dimension": true,
          "constraints": {
            "enum": ["Number", "Percent"]
         }
        }, {
          "name": "Variable",
          "type": "string",
          "dimension": true,
          "constraints": {
            "enum": ["Health Insurance", "Margins of Error"]
         }
        }, {
          "name": "Value",
          "type": "number",
          "dimension": false
        }]
      }
    }
  ],
  "last_modified": "01/14/2019",
  "ckan_extras": {
